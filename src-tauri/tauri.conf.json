{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "mupdf-convert-kit",
  "version": "0.1.0",
  "identifier": "com.mupdf.dev",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:4000",
    "beforeDevCommand": "pnpm run dev",
    "beforeBuildCommand": "pnpm run build"
  },
  "app": {
    "windows": [
      {
        "title": "MuPdf-Convert-Kit",
        "width": 800,
        "minWidth": 375,
        "minHeight": 500,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "hiddenTitle": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}